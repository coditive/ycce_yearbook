<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph3"
    app:startDestination="@id/fragmentSem">

    <fragment
        android:id="@+id/fragmentSem"
        android:name="com.syrous.ycceyearbook.ui.semester.FragmentSem"
        android:label="FragmentSem"
        tools:layout="@layout/fragment_semester">
        <argument
            android:name="department_object"
            app:argType=".ui.home.Department"
            app:nullable="false"/>
        <action
            android:id="@+id/action_fragmentSem_to_fragmentPaperAndResource"
            app:destination="@id/fragmentPaperAndResource">
            <argument
                android:name="subject"
                app:argType=".model.Subject"
                app:nullable="false"/>
        </action>
    </fragment>
    <fragment
        android:id="@+id/fragmentPaperAndResource"
        android:name="com.syrous.ycceyearbook.ui.papers_and_resources.FragmentPaperAndResource"
        android:label="FragmentPaperAndResource"
        tools:layout="@layout/fragment_paper_and_resources">
        <argument
            android:name="subject"
            app:argType=".model.Subject"
            app:nullable="false"/>
        <action
            android:id="@+id/action_fragmentPaperAndResource_to_fragmentPdfRenderer"
            app:destination="@id/fragmentPdfRenderer">
            <argument
                android:name="pdfRef"
                app:argType="java.io.File"
                app:nullable="false"/>
        </action>
    </fragment>

    <fragment
        android:id="@+id/fragmentPdfRenderer"
        android:name="com.syrous.ycceyearbook.ui.pdf_screen.FragmentPdfRenderer"
        android:label="FragmentPdfRenderer"
        tools:layout="@layout/fragment_pdf_renderer">
        <argument
            android:name="pdfRef"
            app:argType="java.io.File"
            app:nullable="false"/>
    </fragment>

</navigation>